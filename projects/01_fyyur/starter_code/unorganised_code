# Initiate a db cluster
initdb -D db


# Start server
pg_ctl start -l db_log.file -D db_dir

# Create a db for project
createdb fyyur

# Migration
flask db init
flask db migrate -m "init fyyur db"
flask db upgrade


# Insert records
INSERT INTO "Artist" (id, name) VALUES ('4', 'Guns N Petals');
